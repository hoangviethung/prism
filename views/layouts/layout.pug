block var

<!DOCTYPE html>
html(lang="en")
	head
		// *** META *** //
		include meta
		include css
		// *** CSS CUSTOM PAGE *** //
		block css
		// *** FAVICON *** //
		link(rel="shortcut icon", href="favicon.ico", type="image/x-icon")
		// *** TITLE *** //
		title #{pageTitle}
	body(class=`${bodyClass}`)
		// *** HEADER *** //
		block header
			include header
		// *** MAIN *** //
		main
			block main
		// *** OVERLAY *** //
		#overlay
		// *** FOOTER *** //
		block footer
			include footer
		include js
		// *** JS CUSTOM PAGE *** //
		block js