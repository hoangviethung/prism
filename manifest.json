{
    "name": "Prism",
    "description": "Prism Walle",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "/js/main.js"
    },
    "action": {
        "default_icon": {
            "16": "images/icon16.png",
            "24": "images/icon24.png",
            "32": "images/icon32.png"
        },
        "default_title": "Open Prism Walle",
        "default_popup": "index.html"
    },
    "permissions": ["storage"]
}
